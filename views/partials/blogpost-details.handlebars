<div class="row">
  <div class="col s12 m5">
    <div class="card-panel teal">
      <h4 class="details-card-title">{{blogpost.title}}</h4>
      <h5 class="card-author white-text">Authored by:{{user.username}}, Date:{{created_at}}
        <a href="/blogposts/{{blogpost.id}}" {{comment.length}}</a>
      </h5>
      <span class="card-article">{{blogpost.article}}</span>
    </div>
  </div>
</div>

